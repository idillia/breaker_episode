{
  "name": "breaker.episode",
  "version": "1.0.0",
  "description": "breaker.episode",
  "main": "src/server.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "pm2 start server/server.js --watch --interpreter babel-node --name appDev && pm2 flush && pm2 logs",
    "web": "webpack -wd",
    "wp": "webpack -p",
    "np": "babel src -d build --copy-files",
    "build": "webpack -p && babel src -d build/src --copy-files && babel server -d build/server --copy-files",
    "prod": "NODE_ENV=production pm2 start build/server/server.js --name appProd && pm2 flush && pm2 logs",
    "dd": "pm2 delete appDev",
    "dp": "pm2 delete appProd"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ]
  },
